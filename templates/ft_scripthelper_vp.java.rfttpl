	/**
	 * Locate and return the verification point %helper:vpName% object in the SUT.
	 */
	protected IFtVerificationPoint %helper:vpName%VP() 
	{
		return vp("%helper:vpName%");
	}
	protected IFtVerificationPoint %helper:vpName%VP(TestObject anchor)
	{
		return vp("%helper:vpName%", anchor);
	}
	
