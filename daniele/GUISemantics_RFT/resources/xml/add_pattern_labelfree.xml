<?xml version="1.0" encoding="UTF-8"?>
<pattern xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="../xml_schemas/Pattern_schema.xsd"
	alloy="ADD.als">
	<window id="initial" card="one" alloy="Initial">
		<title>.*</title>
		<modal>any</modal>
		<root>any</root>
		<action_widget id="aw1" card="one" alloy="Trigger">
			<label>.*</label>
		</action_widget>
		<selectable_widget id="sw1" card="one" alloy="List">
			<label>.*</label>
			<size>.*</size>
		</selectable_widget>
	</window>
	<window id="form_1" card="one" alloy="Form">
		<title>.*</title>
		<modal>any</modal>
		<root>any</root>
		<action_widget id="aw2" card="one" alloy="Ok">
			<label>.*</label>
		</action_widget>
		<action_widget id="aw3" card="one" alloy="Cancel">
			<label>.*</label>
		</action_widget>
		<input_widget id="iw1" card="some" alloy="Input">
			<label>.*</label>
			<text>.*</text>
		</input_widget>
	</window>
	<window id="form_2" card="set" alloy="Form">
		<title>.*</title>
		<modal>any</modal>
		<root>any</root>
		<action_widget id="aw4" card="one" alloy="Ok">
			<label>.*</label>
		</action_widget>
		<action_widget id="aw5" card="one" alloy="Cancel">
			<label>.*</label>
		</action_widget>
		<input_widget id="iw2" card="some" alloy="Input">
			<label>.*</label>
			<text>.*</text>
		</input_widget>
	</window>
	<window id="confirmation" card="lone" alloy="Confirm">
		<title>.*</title>
		<modal>true</modal>
		<root>any</root>
		<action_widget id="aw6" card="one" alloy="Ok">
			<label>.*</label>
		</action_widget>
		<action_widget id="aw7" card="one" alloy="Cancel">
			<label>.*</label>
		</action_widget>
		<input_widget id="iw2" card="none" alloy="">
			<label>.*</label>
			<text>.*</text>
		</input_widget>
	</window>
	<edge>
		<from>aw1</from>
		<to>form_1</to>
	</edge>
	<edge>
		<from>aw3</from>
		<to>initial</to>
	</edge>
	<edge>
		<from>aw2</from>
		<to>form_2</to>
		<to>confirmation</to>
	</edge>
	<edge>
		<from>aw4</from>
		<to>form_2</to>
		<to>confirmation</to>
	</edge>
	<edge>
		<from>aw5</from>
		<to>form_2</to>
		<to>form_1</to>
	</edge>
	<edge>
		<from>aw7</from>
		<to>form_2</to>
		<to>form_1</to>
	</edge>
</pattern>
