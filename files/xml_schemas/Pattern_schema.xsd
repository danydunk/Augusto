<?xml version="1.0" encoding="UTF-8" ?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:element name="pattern">
		<xs:complexType>
			<xs:sequence>
				<xs:sequence minOccurs="0" maxOccurs="unbounded">
					<xs:element name="window">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="title" type="xs:string" />
								<xs:element name="modal" type="xs:string" />
								<xs:element name="root" type="xs:string" />
								<xs:sequence minOccurs="0" maxOccurs="unbounded">
									<xs:element name="action_widget">
										<xs:complexType>
											<xs:sequence>
												<xs:element name="label" type="xs:string" />
											</xs:sequence>
											<xs:attribute name="id" type="xs:string" />
											<xs:attribute name="card" type="xs:string" />
											<xs:attribute name="alloy" type="xs:string" />
										</xs:complexType>
									</xs:element>
								</xs:sequence>
								<xs:sequence minOccurs="0" maxOccurs="unbounded">
									<xs:element name="selectable_widget">
										<xs:complexType>
											<xs:sequence>
												<xs:element name="label" type="xs:string" />
												<xs:element name="size" type="xs:string" />
											</xs:sequence>
											<xs:attribute name="id" type="xs:string" />
											<xs:attribute name="card" type="xs:string" />
											<xs:attribute name="alloy" type="xs:string" />
										</xs:complexType>
									</xs:element>
								</xs:sequence>
								<xs:sequence minOccurs="0" maxOccurs="unbounded">
									<xs:element name="input_widget">
										<xs:complexType>
											<xs:sequence>
												<xs:element name="label" type="xs:string" />
												<xs:element name="value" type="xs:string" />
											</xs:sequence>
											<xs:attribute name="id" type="xs:string" />
											<xs:attribute name="card" type="xs:string" />
											<xs:attribute name="alloy" type="xs:string" />
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:sequence>
							<xs:attribute name="id" type="xs:string" />
							<xs:attribute name="card" type="xs:string" />
							<xs:attribute name="alloy" type="xs:string" />
						</xs:complexType>
					</xs:element>
				</xs:sequence>
				<xs:sequence minOccurs="0" maxOccurs="unbounded">
					<xs:element name="edge">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="from" type="xs:string" />
								<xs:sequence minOccurs="1" maxOccurs="unbounded">
									<xs:element name="to" type="xs:string" />
								</xs:sequence>
							</xs:sequence>
							<xs:attribute name="id" type="xs:string" />
							<xs:attribute name="type" type="xs:string" />
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:sequence>
			<xs:attribute name="id" type="xs:string" />
			<xs:attribute name="alloy" type="xs:string" />
		</xs:complexType>
	</xs:element>
</xs:schema>